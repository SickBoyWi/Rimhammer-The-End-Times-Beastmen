<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="BuildingBase" Name="RH_TET_Beastmen_FabricDoorBase" Abstract="True">
		<thingClass>Building_Door</thingClass>
		<category>Building</category>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<fillPercent>1</fillPercent>
		<useHitPoints>true</useHitPoints>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<selectable>true</selectable>
		<tickerType>Normal</tickerType>
		<rotatable>false</rotatable>
		<soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Structure</designationCategory>
		<holdsRoof>true</holdsRoof>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<drawerType>RealtimeOnly</drawerType>
		<building>
			<isInert>true</isInert>
			<canPlaceOverWall>true</canPlaceOverWall>
			<soundDoorOpenPowered>Door_OpenPowered</soundDoorOpenPowered>
			<soundDoorClosePowered>Door_ClosePowered</soundDoorClosePowered>
			<soundDoorOpenManual>Door_OpenManual</soundDoorOpenManual>
			<soundDoorCloseManual>Door_CloseManual</soundDoorCloseManual>
			<blueprintClass>Blueprint_Door</blueprintClass>
			<blueprintGraphicData>
				<texPath>Things/Building/Structure/RH_TET_Beastmen_TentDoorBlueprint</texPath>
			</blueprintGraphicData>
			<ai_chillDestination>false</ai_chillDestination>
		</building>
		<researchPrerequisites>
			<li>RH_TET_Beastmen_WallsDoors</li>
		</researchPrerequisites>
		<damageMultipliers>
			<li>
				<damageDef>Bomb</damageDef>
				<multiplier>2</multiplier>
			</li>
			<li>
				<damageDef>Flame</damageDef>
				<multiplier>2</multiplier>
			</li>
		</damageMultipliers>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_DoorLearnOpeningSpeed</li>
		</placeWorkers>
	</ThingDef>

	<ThingDef ParentName="RH_TET_Beastmen_FabricDoorBase">
		<defName>RH_TET_Beastmen_CrudeDoor</defName>
		<label>Crude tent flap</label>
		<description>A leather or fabric door that divides rooms. Fast to build and open, but very weak.</description>
		<statBases>
			<WorkToBuild>20</WorkToBuild>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Structure/RH_TET_Beastmen_TentDoorMover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Structure/RH_TET_Beastmen_TentDoorMenuIcon</uiIconPath>
		<costStuffCount>10</costStuffCount>
		<designationHotKey>Misc1</designationHotKey>
	</ThingDef>

	<ThingDef ParentName="BuildingBase" Name="TentWall">
		<defName>RH_TET_Beastmen_CrudeWall</defName>
		<label>Crude tent wall</label>
		<description>An wall make of leather or fabric. Capable of holding up a roof. Easy to build, although it's weak and gives no protection from ranged fire.</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Building/Linked/RH_TET_Beastmen_TentWall_Atlas</texPath>
			<drawSize>(1,1)</drawSize>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Wall</li>
				<li>Rock</li>
			</linkFlags>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Linked/RH_TET_Beastmen_TentMenuIcon</uiIconPath>
		<statBases>
			<MaxHitPoints>35</MaxHitPoints>
			<WorkToBuild>5</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<costStuffCount>5</costStuffCount>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>1</fillPercent>
		<coversFloor>true</coversFloor>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<holdsRoof>true</holdsRoof>
		<designationCategory>Structure</designationCategory>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<canOverlapZones>false</canOverlapZones>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<building>
			<isInert>true</isInert>
			<ai_chillDestination>false</ai_chillDestination>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/RH_TET_Beastmen_TentWallBlueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<researchPrerequisites>
			<li>RH_TET_Beastmen_WallsDoors</li>
		</researchPrerequisites>
		<damageMultipliers>
			<li>
				<damageDef>Bomb</damageDef>
				<multiplier>2</multiplier>
			</li>
			<li>
				<damageDef>Flame</damageDef>
				<multiplier>2</multiplier>
			</li>
		</damageMultipliers>
		<designationHotKey>Misc3</designationHotKey>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>RH_TET_Beastmen_Vent</defName>
		<label>crude vent</label>
		<description>A crude looking vent for equalizing the temperature between two rooms without allowing people to walk between them. Crude beastman construction.</description>
		<thingClass>Building_Vent</thingClass>
		<graphicData>
			<texPath>Things/Building/Structure/RH_TET_Beastmen_Vent</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<coversFloor>true</coversFloor>
		<blockLight>true</blockLight>
		<castEdgeShadows>true</castEdgeShadows>
		<canOverlapZones>false</canOverlapZones>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<statBases>
			<WorkToBuild>300</WorkToBuild>
			<MaxHitPoints>70</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Beauty>0</Beauty>
		</statBases>
		<tickerType>Rare</tickerType>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<costList>
			<WoodLog>10</WoodLog>
		</costList>
		<fillPercent>0.99</fillPercent>
		<altitudeLayer>PawnUnused</altitudeLayer>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<placeWorkers>
			<li>PlaceWorker_Vent</li>
			<li>TheEndTimes_Beastmen.PlaceWorker_OnWallVent</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<building>
			<canPlaceOverWall>true</canPlaceOverWall>
			<isEdifice>false</isEdifice>
		</building>
		<clearBuildingArea>false</clearBuildingArea>
		<researchPrerequisites>
			<li>RH_TET_Beastmen_WallsDoors</li>
		</researchPrerequisites>
		<designationHotKey>Misc8</designationHotKey>
		<comps>
			<li Class="CompProperties_Flickable">
				<commandTexture>UI/Commands/Vent</commandTexture>
				<commandLabelKey>CommandDesignateOpenCloseVentLabel</commandLabelKey>
				<commandDescKey>CommandDesignateOpenCloseVentDesc</commandDescKey>
			</li>
		</comps>
		<designationCategory>Temperature</designationCategory>
		<disableImpassableShotOverConfigError>True</disableImpassableShotOverConfigError>
	</ThingDef>
  
</Defs>
