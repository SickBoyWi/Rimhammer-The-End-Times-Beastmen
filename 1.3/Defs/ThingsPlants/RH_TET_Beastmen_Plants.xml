<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="TreeBase">
		<defName>RH_TET_Beastmen_GreatOak</defName>
		<label>great oak</label>
		<description>A massive and very slow growing tree.</description>
		<graphicData>
			<texPath>Things/Plants/GreatOak</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.55, 0.2, 0.2)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<statBases>
			<Beauty>15</Beauty>
			<MaxHitPoints>400</MaxHitPoints>
			<Flammability>0.65</Flammability>
		</statBases>
		<blockLight>true</blockLight>
		<plant>
			<fertilityMin>0.8</fertilityMin>
			<harvestedThingDef>WoodLog</harvestedThingDef>
			<fertilitySensitivity>0.8</fertilitySensitivity>
			<leaflessGraphicPath>Things/Plants_Leafless/GreatOak_Leafless</leaflessGraphicPath>
			<growDays>300</growDays>
			<harvestWork>2000</harvestWork>
			<harvestYield>150</harvestYield>
			<wildClusterWeight>8</wildClusterWeight>
			<wildClusterRadius>10</wildClusterRadius>
			<visualSizeRange>8~12</visualSizeRange>
			<sowTags/>
		</plant>
	</ThingDef>
	
